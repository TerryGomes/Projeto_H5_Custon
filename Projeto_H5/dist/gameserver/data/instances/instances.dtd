		count CDATA #IMPLIED
		mobId CDATA #REQUIRED
		respawn CDATA #IMPLIED
		respawnRnd CDATA #IMPLIED
		type CDATA #REQUIRED>
	<!ELEMENT coords (#PCDATA)>
	<!ATTLIST coords
		loc CDATA #REQUIRED>

<!ELEMENT group (#PCDATA)>
<!ATTLIST group
	name CDATA #REQUIRED
