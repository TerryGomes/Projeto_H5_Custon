	<!ELEMENT level (#PCDATA)>
	<!ATTLIST level
		max CDATA #REQUIRED
		min CDATA #REQUIRED>
	<!ELEMENT party (#PCDATA)>
	<!ATTLIST party
		max CDATA #REQUIRED
		min CDATA #REQUIRED>
	<!ELEMENT return (#PCDATA)>
	<!ATTLIST return
		loc CDATA #REQUIRED>
	<!ELEMENT teleport (#PCDATA)>
	<!ATTLIST teleport
		loc CDATA #REQUIRED>
		<!ELEMENT remove (#PCDATA)>
